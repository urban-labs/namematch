    "InteractiveShell.ast_node_interactivity = \"all\"\n",
    "from IPython.core.display import display, Markdown\n",
    "\n",
    "pd.set_option('display.max_columns', None)\n",
    "pd.set_option('display.max_rows', None)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
