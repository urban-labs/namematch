  {
   "cell_type": "code",
   "execution_count": 34,
   "metadata": {},
   "outputs": [],
   "source": [
    "answer_key = pd.read_csv('raw_data/answer_key.csv')\n",
    "\n",
    "matched_potential_candidates = pd.merge(\n",
    "    matched_potential_candidates, \n",
    "    answer_key,\n",
    "    on=[col for col in answer_key.columns if col != 'candidate_id']\n",
    ")"
   ]
